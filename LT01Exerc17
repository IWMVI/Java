// Calcule a quantidade de litros gastos em uma viagem, sabendo que o automóvel faz 12Km/l.
// Receber o tempo de percurso e a velocidade média.

import javax.swing.JOptionPane;

public class Main {
	public static void main (String [] arg) {
		int kml = 12;
		double km, vmd, gasto, temp;
		
		temp = Double.parseDouble(JOptionPane.showInputDialog ("Qual o tempo de viagem? "));
		vmd = Double.parseDouble(JOptionPane.showInputDialog ("Qual a velocidade média na viagem? "));
		
		km = temp * vmd;
		gasto = km/kml;
		
		System.out.println ("O total de combustível gasto foi: " +gasto+ " litros");
	}
}
