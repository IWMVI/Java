// Receba 2 números reais, calcule e mostre a diferença entre eles.

import javax.swing.JOptionPane;

public class Main {
	public static void main (String [] arg) {
		int a,b,r;
		
		a = Integer.parseInt(JOptionPane.showInputDialog("Qual o valor de A? "));
		b = Integer.parseInt(JOptionPane.showInputDialog("Qual o valor de B? "));
		
		r = (a-b); 
		
		System.out.print ("A diferença entre os números é: " +r);
	}
}
