// Receba 2 números inteiros, calcule e mostre a soma dos quadrados.

import javax.swing.JOptionPane;

public class LT01EXERC09 {
	public static void main (String [] arg) {
	
		int a,b,r;
		
		a = Integer.parseInt (JOptionPane.showInputDialog("Qual o valor de A? "));
		b = Integer.parseInt(JOptionPane.showInputDialog("Qual o valor de B? "));
		
		r = (a*a)+(b*b);
		
		System.out.print("A soma dos quadrados é: " +r);
		
	}
}
